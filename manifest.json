{
  "manifest_version": 3,
  "name": "Go Home",
  "version": "1.0",
  "description": "Navigate to the top-level domain of the current page",
  "permissions": ["activeTab"],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Go to top-level domain"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
